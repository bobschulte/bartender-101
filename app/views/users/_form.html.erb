<%= form_with model: @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br><br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br><br>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br><br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= f.label :image_url%>
  <%= f.text_field :img_url%>
  <br><br>
  <%= f.submit %>
<% end %>
